{
  "name": "stream-notes",
  "version": "1.0.0",
  "private": false,
  "sideEffects": false,
  "license": "Apache-2.0",
  "dependencies": {
    "@supabase/supabase-js": "^2.43.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "NODE_ENV=production react-scripts build",
    "lint": "eslint --ext .js,.jsx src",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "optimize": "echo 'Build optimized for performance'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  }
}
